<div class="serch_form" >
  <table align="center">
    <tr>
      <div class="serch_form">
      <%= form_with url: tagsearches_search_path,method: :get, local: true do |f| %>
        <% category = Book.select(:category).distinct %> 
        <%= f.collection_select(:word, category, :category, :category) %>
      <td><%= f.submit'タグ検索' %></td>
      <% end %>
      </div>
    </tr> 
  </table>  
</div>